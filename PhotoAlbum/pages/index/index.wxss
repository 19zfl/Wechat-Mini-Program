.top {
	width: 100%;
	height: 300rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: space-evenly;
	background: linear-gradient(to right bottom, #002fa7, white);
}
.title {
	font-size: 60rpx;
	color: white;
}
.titlee {
	color: whitesmoke;
	font-size: 50rpx;
	margin-top: 20rpx;
}
.photoBox .photo {
	width: 250rpx;
	height: 250rpx;
	float: left;
}
.photoBox .photo image {
	width: 100%;
	height: 100%;
}
.upload {
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	font-size: 80rpx;
	background-color: thistle;
	position: fixed;
	right: 60rpx;
	bottom: 60rpx;
	text-align: center;
	line-height: 90rpx;
}